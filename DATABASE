CREATE DATABASE Store;

CREATE TABLE Products (
    P_ID INT PRIMARY KEY AUTO_INCREMENT,
    P_Name VARCHAR(50) NOT NULL,
    P_Price INT NOT NULL,
    P_Stock INT NOT NULL,
    P_Expiration_Date DATE NOT NULL
);

